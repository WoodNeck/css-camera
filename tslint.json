{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rulesDirectory": [
      "tslint-consistent-codestyle",
      "tslint-eslint-rules"
    ],
    "rules": {
      "quotemark": [true, "single"],
      "variable-name": false
    },
    "linterOptions": {
      "exclude": [
        "node_modules/**/*"
      ]
    }
  }
